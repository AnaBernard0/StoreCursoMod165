@model StoreCursoMod165.Models.Product

<h1>@SharedLocalizer["Delete"]</h1>

<h3 class="text-danger">@SharedLocalizer["Are you sure you want to delete this Product?"]</h3>

<hr />

<div class="row">
    <div class="col-md-4">

        <form asp-action="Delete">
            <partial name="Shared/_form" />

            <div class="form-group">
                <input type="submit" value=@SharedLocalizer["Delete"] class="btn btn-danger" />
                <a asp-action="Index">@SharedLocalizer["Back to List"]</a>
            </div>
        </form>
    </div>
</div>